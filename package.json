{
  "name": "create-folder-react-mcy",
  "displayName": "create-folder-react-mcy",
  "description": "create folder for react",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "repository": {
		"type": "git",
		"url": "https://github.com/mochenyin/create-folder-react.git"
	},
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "create-folder-react-mcy.create",
        "title": "create-folder-react-mcy: create"
      }
    ],
    "menus": {
			"explorer/context": [
				{
					"command": "create-folder-react-mcy.create",
					"when": "resourceScheme == 'file'"
				}
			]
      
		}
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.80.0",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "eslint": "^8.34.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^4.9.5",
    "@vscode/test-electron": "^2.2.3"
  }
}
